{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"declaration": true,
		"inlineSourceMap": false,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"moduleResolution": "node",
		"outDir": "lib",
		"forceConsistentCasingInFileNames": true,
		"lib": ["es2016", "dom"]
	},
	"include": ["src"]
}
